<!DOCTYPE html>
<html lang="zh" >
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <title>{{ title }}</title>
    {% block globalMandatoryStylesheet %}{% endblock %}
    {% block pagePluginStylesheet %}{% endblock %}
    {% block themeGlobalStylesheet %}{% endblock %}
    {% block themeLayoutStylesheet %}{% endblock %}
    {% block pageStylesheet %}{% endblock %}
    {% block headGlobalJavascript %}{% endblock %}

</head>
<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">

<div class="page-header navbar navbar-fixed-top">

    <div class="page-header-inner ">

        <div class="page-logo">
            <a href="index.html">
                <img src="{{ asset('bundles/Ui/layout/img/logo.png') }}" alt="logo" class="logo-default" /> </a>
            <div class="menu-toggler sidebar-toggler"> </div>
        </div>
        <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse"> </a>
        <div class="top-toolbar">
            {% include 'UiBundle:Common:top-toolbar.html.twig' %}
        </div>
        <div class="top-menu">
            {% include 'UiBundle:Common:tinc.html.twig' %}
        </div>
    </div>
</div>

<div class="clearfix"> </div>

<div class="page-container">
    <div class="page-sidebar-wrapper">
        <div class="page-sidebar navbar-collapse collapse">
            {% include 'UiBundle:Common:linc.html.twig' %}
        </div>
    </div>
    <div class="page-content-wrapper">
        <div class="page-content">
            {% include 'UiBundle:Common:contabs.html.twig' %}
            {% block main %}{% endblock %}
        </div>
    </div>
</div>

{% block footer %}{% endblock %}

<!--[if lt IE 9]>
<script src="{{ asset('bundles/Ui/global/plugins/respond.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/Ui/global/plugins/excanvas.js') }}" type="text/javascript"></script>
<![endif]-->

{% block rawJavascript %}{% endblock %}
{% block corePluginsJavascript %}{% endblock %}
{% block pagePluginsJavascript %}{% endblock %}
{% block themeGlobalJavascript %}{% endblock %}
{% block pageLevelJavascript %}{% endblock %}
{% block themeLayoutJavascript %}{% endblock %}

</body>
</html>